<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="All" verbose="1"  parallel="classes" thread-count="2" >
    <test name="navigation"   >
        <packages>
            <package name=".*" />
        </packages>
    </test>

    <listeners>
        <listener class-name="com.sergiuchuckmisha.bf.testng.TakeScreenshotListener"/>
    </listeners>
</suite>
